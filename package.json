{
  "name": "dyno-config",
  "version": "0.1.0-beta.2",
  "description": "dynamic config that can be updated in runtime",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.js --outdir ./dist --target node",
    "test": "bun test",
    "publish": "npm publish",
    "prepublishOnly": "bun run build"
  },
  "keywords": ["config", "dynamic config", "runtime config"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ioredis": "^5.3.2",
    "yaml": "^2.3.4",
    "strip-json-comments": "^5.0.1"
  },
  "devDependencies": {
    "@types/bun": "^1.2.9",
    "@types/ioredis": "^5.0.0"
  }
}